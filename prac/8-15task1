	<h2><a name="text">2 text</a></h2>
	<!--格式化文本-->
	<b>	bold	</b>	<br />
	<strong>	strong	</strong>	<br />
	<big>	big	</big>	<br />
	<em>	emphasize	</em>	<br />
	<i>	italic	</i>	<br />
	<small>	small	</small>	<br />
	this <sub>	subscript	</sub>	<br />
	this <sup>	superscript	</sup>	

	<!--pre 保留空格和换行-->
	<pre>
	while(!0)
		i++
	</pre>	
	<!--显示计算机代码-->
	<code>	code	</code>	<br />
	<kbd>	keyboard input	</kbd> <br />
	<tt>	teletype	</tt>	<br />
	<samp>	sample	</samp>	<br />
	<var>	variable	</var>	<br />

